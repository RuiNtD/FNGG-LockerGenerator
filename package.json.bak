{
  "main": "./src/main.ts",
  "scripts": {
    "start": "bun .",
    "compile": "bun compileWindows && bun compileMac && bun compileLinux",
    "compileWindows": "FNGGLG_COMPILED=true bun build --compile --target=bun-windows-x64 --minify --sourcemap --bytecode --windows-icon=icon.ico ./src/main.ts --outfile out/FNGGLockerGenerator",
    "compileMac": "deno compile --target x86_64-apple-darwin -o out/FNGGLockerGenerator-Mac -A src/main.ts --compiled",
    "compileLinux": "deno compile --target x86_64-unknown-linux-gnu -o out/FNGGLockerGenerator-Linux -A src/main.ts --compiled"
  },
  "dependencies": {
    "@cross/dir": "npm:@jsr/cross__dir",
    "@inquirer/prompts": "^7.2.3",
    "@ruintd/inquirer-enter": "npm:@jsr/ruintd__inquirer-enter",
    "@std/async": "npm:@jsr/std__async",
    "@std/fmt": "npm:@jsr/std__fmt",
    "@tsconfig/bun": "^1.0.7",
    "@types/bun": "^1.1.18",
    "axios": "^1.7.9",
    "chalk": "^5.4.1",
    "ora": "^8.1.1",
    "p-memoize": "^7.1.1",
    "valibot": "^1.0.0-beta.14"
  }
}
